    "keyboard": "handwired/dactyl_manuform_almang",
    "keymap": "default",
    "layout": "LAYOUT_5x6",
    "config": {
        "encoder_map": true,
        "encoder_enable": true,
        "oled_enable": true,
        "rgblight_enable": true
