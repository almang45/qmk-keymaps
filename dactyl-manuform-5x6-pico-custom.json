                "snake": true,
                "knight": true,
                "christmas": true,
                "static_gradient": true,
                "rgb_test": true,
                "alternating": true,
                "twinkle": true
            }
        },
        "ws2812": {
            "pin": "GP7"
        },
        "split": {
            "enabled": true,
            "handedness": {
                "pin": "GP22"
            },
            "serial": {
                "driver": "vendor",
                "pin": "GP1"
            },
            "transport": {
                "watchdog": true
            }
